{
  "name": "TimeTracker",
  "version": "0.1",
  "description": "Build an Extension!",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "tabs",
    "activeTab",
    "idle",
    "storage",
    "unlimitedStorage"
  ],
  "background": {
    "scripts": [
      "background.js",
      "data.js"
    ]
  }
}